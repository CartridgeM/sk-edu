name: learn-github-actions
reun-name: ${{ github.actor }} is learning Github Actions
on: [push]
jobs:
  check-bats-version:
    runs-on: ubuntu-latest
    steaps:
    - uses: actions/checkout@v3
    - uses: actions/setup-node@v3
      with:
        node-version:'14'
    -run :npm install -g bats
    -run: bats -v
